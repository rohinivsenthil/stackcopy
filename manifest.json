{
  "manifest_version": 2,
  "name": "stackcopy",
  "version": "0.0",
  "description": "Copy code-snippets from stack exchange sites",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.stackexchange.com/*",
        "*://askubuntu.com/*",
        "*://mathoverflow.net/*",
        "*://serverfault.com/*",
        "*://stackapps.com/*",
        "*://stackexchange.com/*",
        "*://stackoverflow.com/*",
        "*://superuser.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}